{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${fabric_mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${mod_display_url}",
    "sources": "${mod_display_url}",
    "issues": "${mod_issue_tracker}"
  },
  "license": "${mod_license}",
  "icon": "${mod_id}.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.gallon.fabric.HorseStatsModFabric"
    ],
    "modmenu": [
      "dev.gallon.fabric.config.ModMenu"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_requirement}",
    "minecraft": "${fabric_minecraft_version_requirement}",
    "java": "${fabric_java_version_requirement}",
    "fabric-api": "${fabric_api_version_requirement}",
    "cloth-config": "${fabric_cloth_config_version_requirement}"
  }
}
